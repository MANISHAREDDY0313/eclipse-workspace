package oopspractise;

 class Test {
	Test(double d) {
		this(10);
		System.out.println("double arg constructor");
	}

	Test(int i) {
		this();
		System.out.println("int arg constructor");

	}

	Test() {
		this();
		System.out.println("int arg constructor");
	}
public static void main(String[] args) {
	Test t1 = new Test(10.5);
}
}
